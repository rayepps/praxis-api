    "make-manifest": "ts-node src/ci/manifest.ts",
    "importer": "source ./env.sh && ts-node src/dev/importer.ts",
    "seed": "source ./env.sh && ts-node src/dev/seed.ts",
    "demo": "source ./env.sh && ts-node ./src/dev/demo.ts"
  },
